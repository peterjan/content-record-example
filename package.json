{
  "name": "content-record-example-skapp",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "env": "web",
  "browserslist": [
    "defaults",
    "not IE 11",
    "not OperaMini all"
  ],
  "scripts": {
    "serve:dev": "rm dist/main.js && npx webpack && serve dist",
    "build": "npx webpack",
    "lint:eslint": "eslint --ext .ts src",
    "lint:tsc": "tsc --noemit",
    "start": "webpack serve --open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --watch"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --ext .ts --fix",
      "tsc --esModuleInterop --noemit",
      "prettier --write"
    ],
    "*.{json,yml,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/url-join": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.11.0",
    "eslint-plugin-jsdoc": "^31.0.7",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.16",
    "typescript": "^4.1.3",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "post-me": "^0.4.4",
    "skynet-js": "^3.0.1-beta",
    "url-join": "^4.0.1"
  }
}
